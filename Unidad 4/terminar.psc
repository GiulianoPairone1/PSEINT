Proceso sin_titulo
	Definir IDS, matematica, algebra Como Entero;
	Definir i, j Como Entero
	Dimension matematica[10];
	Dimension  algebra[10];
	Mostrar "Ingrese los IDS de los alumnos pertenecientes a Matematica";
	Para i<-0 Hasta 9 Con Paso 1 Hacer
		Leer  matematica[i];
	Fin Para
	Mostrar "Ingrese los IDS de los laumnos pertenecientes a algebra";
	Para j<-0 Hasta 9 Con Paso 1 Hacer
		Leer algebra[j];
	Fin Para
	
FinProceso
